RewriteEngine On

RewriteRule ^([a-zA-Z_-]+)$ index.php?p=$1/index

RewriteRule ^([a-zA-Z_-]+)/$ index.php?p=$1/index

RewriteRule ^([a-zA-Z_-]+)/([a-zA-Z_-]+)$ index.php?p=$1/$2

RewriteRule ^([a-zA-Z_-]+)/([a-zA-Z_-]+)/([0-9]+)$ index.php?p=$1/$2&id=$3

RewriteRule ^([a-zA-Z_-]+)/([a-zA-Z_-]+)/([0-9]+)/$ index.php?p=$1/$2&id=$3

RewriteRule ^([a-zA-Z_-]+)/([a-zA-Z_-]+)/([0-9]+)/([0-9]+)$ index.php?p=$1/$2&id=$3&id2=$4